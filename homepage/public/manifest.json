  "shortcuts": [
    {
      "name": "Browse Creators",
      "short_name": "Browse",
      "description": "Find your favorite Haitian celebrities",
      "url": "/browse",
      "icons": [{ "src": "/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your video message orders",
      "url": "/creator/dashboard",
      "icons": [{ "src": "/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Admin Dashboard",
      "short_name": "Admin",
      "description": "Mobile admin dashboard for platform management",
      "url": "/admin/mobile",
      "icons": [{ "src": "/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Emergency Response",
      "short_name": "Emergency",
      "description": "Quick access to emergency admin controls",
      "url": "/admin/mobile#emergency",
      "icons": [{ "src": "/icon-96x96.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+annpale",
      "url": "/admin/mobile?action=%s"
    }
  ],
  "share_target": {
    "action": "/admin/mobile",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "screenshots": [
    {
      "src": "/screenshot-mobile-1.png",
      "sizes": "640x1136",
      "type": "image/png",
      "platform": "narrow",
      "label": "Mobile admin dashboard overview"
    },
    {
      "src": "/screenshot-mobile-2.png",
      "sizes": "640x1136",
      "type": "image/png",
      "platform": "narrow",
      "label": "Emergency response controls"
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "offline_enabled": true,
  "features": [
    "biometric-auth",
    "offline-mode",
    "push-notifications",
    "background-sync"
  ]
}